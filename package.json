{
  "name": "static-test",
  "version": "0.0.1",
  "description": "KCD Static Tests",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/src/index.js",
    "start-dev": "cd src && ts-node index.ts --inspect=0.0.0.0:9229",
    "start-watch": "nodemon -L src/index.ts --inspect=0.0.0.0:9229 --exec ts-node",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json)\"",
    "test": "jest --watchAll",
    "test-ci": "jest --collect-coverage",
    "publish-code-coverage": "codecov",
    "validate": "npm run lint && npm run build"
  },
  "author": "juanjofp",
  "license": "MIT",
  "devDependencies": {
    "@types/convict": "^5.2.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-filenames": "^1.3.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
